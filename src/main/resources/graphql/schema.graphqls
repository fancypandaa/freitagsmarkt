schema {
    query: Query
#    mutation: Mutation
}

type AdsCommand {
        id: ID!
        daysOfSale: Int
        Status: String
        published: String
        sellerId: Int
        car: Car
}

type Car {
        id: ID!
       model:String!
       city: String!
       days: String!
       price: String!
       mileage: String!
       generation: String!

}

type Query {
    listAds(page:Int,size:Int):[AdsCommand]
    findAdById(id: ID!): AdsCommand
}

